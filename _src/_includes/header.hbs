<header class="wrapper site-header" role="banner">
    <div class="container">
        <nav>
            <ul class="site-nav">
                <li class="nav__item logo">
                    <a href="/" class="nav-link">{{inc src="icon.hbs" icon="logo" class=""}} {{site.title}}</a>
                </li>
                {{#each site.navigation}}
                <li class="nav__item"><a href="{{this.url}}" class="nav-link">{{ this.label }}</a></li>
                {{/each}}
                <li class="nav__item"><a href="" class="nav-link">Contributions</a></li>
                <li class="nav__item nav__item--right">
                    <a href="{{site.links.source}}" class="nav-link" title="Browsersync on Github">{{inc src="icon.hbs"
                    icon="github" class=""}}</a>
                    <a href="{{site.links.twitter}}" class="nav-link" title="@Browsersync on Twitter">{{inc src="icon.hbs"
                    icon="twitter" class=""}}</a>
                </li>
            </ul>
        </nav>
    </div>
</header><!-- .end .site-header -->
